  <!-- ElectricalSteel -->

  <recipe name="Sagmill: ElectricalSteel Ingot" required="false">
    <sagmilling energy="2400" bonus="none">
      <input name="ingotElectricalSteel" />
      <output name="dustElectricalSteel" />
    </sagmilling>
  </recipe>

  <recipe name="Smelting: ElectricalSteel Dust" required="false">
    <smelting>
      <input name="dustElectricalSteel" />
      <output name="ingotElectricalSteel" />
    </smelting>
  </recipe>

  <recipe name="Sagmill: ElectricalSteel Block" required="false">
    <sagmilling energy="3600" bonus="none">
      <input name="blockElectricalSteel" />
      <output name="dustElectricalSteel" amount="9" />
    </sagmilling>
  </recipe>

