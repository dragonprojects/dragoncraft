<?xml version="1.0" encoding="UTF-8"?>
<enderio:recipes xmlns:enderio="http://enderio.com/recipes" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://enderio.com/recipes recipes.xsd ">


  <recipe name="Tank: XP Bottle, emptying" required="true">
    <tanking type="EMPTY" logic="XPBOTTLE">
      <input name="minecraft:experience_bottle"/>
      <fluid name="xpjuice" amount="1"/><!-- for logic=XPBOTTLE amount is a factor to the vanilla amount -->
    </tanking>
  </recipe>

  <recipe name="Tank: XP Bottle, filling" required="true">
    <tanking type="FILL" logic="XPBOTTLE">
      <input name="minecraft:glass_bottle"/>
      <fluid name="xpjuice" amount="2"/><!-- for logic=XPBOTTLE amount is a factor to the vanilla amount -->
      <output name="minecraft:experience_bottle"/>
    </tanking>
  </recipe>

